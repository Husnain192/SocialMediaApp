{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/posts/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/upload/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/chat/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/message/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}