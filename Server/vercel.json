{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/auth/(.*)",
      "destination": "index.js"
    },
    {
      "source": "/user/(.*)",
      "destination": "index.js"
    },
    {
      "source": "/posts/(.*)",
      "destination": "index.js"
    },
    {
      "source": "/upload/(.*)",
      "destination": "index.js"
    },
    {
      "source": "/chat/(.*)",
      "destination": "index.js"
    },
    {
      "source": "/message/(.*)",
      "destination": "index.js"
    }
  ]
}